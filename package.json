{
	"name": "fishfish-js",
	"version": "1.0.0",
	"description": "JavaScript wrapper for the fishfish.gg API",
	"scripts": {
		"test": "vitest run",
		"build:clean": "del-cli dist",
		"build:check": "tsc --emitDeclarationOnly",
		"build:esm": "swc ./src --out-dir ./dist",
		"build": "yarn build:clean && yarn build:check && yarn build:esm",
		"build:test": "yarn build && yarn test",
		"lint": "prettier --check . && eslint src --ext ts",
		"format": "prettier --write . && eslint src --ext ts --fix",
		"fmt": "yarn format",
		"prepack": "yarn build && yarn lint",
		"postinstall": "is-ci || husky install"
	},
	"type": "module",
	"keywords": [
		"fishfish",
		"anti-scam",
		"api",
		"wrapper"
	],
	"author": "fishfish-gg <admin@fishfish.gg>",
	"contributors": [
		"JPBM135 <jpedrobm0@gmail.com>"
	],
	"license": "AGPL-3.0",
	"module": "./dist/index.js",
	"typings": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./types": {
			"types": "./dist/index.d.ts"
		}
	},
	"dependencies": {
		"undici": "^5.15.0",
		"ws": "^8.12.0"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.57",
		"@swc/core": "^1.3.3",
		"@types/node": "^18.7.23",
		"@types/ws": "^8.5.4",
		"del-cli": "^5.0.0",
		"eslint": "^8.32.0",
		"eslint-config-neon": "^0.1.40",
		"husky": "^8.0.3",
		"is-ci": "^3.0.1",
		"lint-staged": "^13.1.0",
		"prettier": "^2.8.3",
		"typescript": "^4.9.4",
		"vitest": "^0.27.1"
	},
	"engines": {
		"node": ">=17.9.0"
	},
	"packageManager": "yarn@3.2.2"
}

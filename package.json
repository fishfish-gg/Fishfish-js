{
	"name": "fishfish-js",
	"version": "1.0.0",
	"description": "",
	"scripts": {
		"test": "vitest run",
		"build:clean": "del-cli dist",
		"build:check": "tsc --emitDeclarationOnly",
		"build:esm": "swc ./src --out-dir ./dist",
		"build": "yarn build:clean && yarn build:check && yarn build:esm",
		"build:test": "yarn build && yarn test",
		"lint": "prettier --check . && eslint src --ext ts",
		"format": "prettier --write . && eslint src --ext ts --fix",
		"fmt": "yarn format",
		"prepack": "yarn build && yarn lint"
	},
	"type": "module",
	"keywords": [
		"fishfish",
		"anti-scam",
		"api",
		"wrapper"
	],
	"author": "JPBM135 <jpedrobm0@gmail.com>",
	"license": "AGPL-3.0",
	"dependencies": {
		"undici": "^5.11.0",
		"ws": "^8.10.0"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.57",
		"@swc/core": "^1.3.3",
		"@types/node": "^18.7.23",
		"@types/ws": "^8.5.3",
		"del-cli": "^5.0.0",
		"eslint": "^8.22.0",
		"eslint-config-neon": "^0.1.33",
		"prettier": "^2.7.1",
		"typescript": "^4.7.4",
		"vitest": "^0.24.3"
	},
	"engines": {
		"node": ">=17.9.0"
	},
	"packageManager": "yarn@3.2.2"
}
